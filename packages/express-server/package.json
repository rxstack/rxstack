{
  "name": "@rxstack/express-server",
  "version": "0.2.0",
  "description": "RxStack ExpressServer Module",
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/rxstack.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "rxstack",
    "express"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "peerDependencies": {
    "@rxstack/async-event-dispatcher": "^0.2",
    "@rxstack/core": "^0.2",
    "@rxstack/exceptions": "^0.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "express": "^4.16.4",
    "injection-js": "^2.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@rxstack/utils": "^0.1.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.35",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^2.2.48",
    "@types/node": "^11.10.5",
    "chai": "3.5.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.4",
    "should": "^13.2.3",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "bugs": {
    "url": "https://github.com/rxstack/rxstack/issues"
  },
  "homepage": "https://github.com/rxstack/rxstack",
  "files": [
    "dist",
    "README.md",
    "./../../LICENSE"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
