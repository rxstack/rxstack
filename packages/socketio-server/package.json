{
  "name": "@rxstack/socketio-server",
  "version": "0.2.1",
  "description": "RxStack SocketIO Server Module",
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/rxstack.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "rxstack",
    "socketio"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "peerDependencies": {
    "@rxstack/async-event-dispatcher": "^0.2",
    "@rxstack/core": "^0.2",
    "@rxstack/exceptions": "^0.2"
  },
  "dependencies": {
    "injection-js": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^2.2.48",
    "@types/node": "^11.13.5",
    "@types/socket.io": "^1.4.40",
    "chai": "3.5.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "should": "^13.2.3",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.3"
  },
  "bugs": {
    "url": "https://github.com/rxstack/rxstack/issues"
  },
  "homepage": "https://github.com/rxstack/rxstack",
  "files": [
    "dist",
    "README.md",
    "./../../LICENSE"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
