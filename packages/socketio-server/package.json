{
  "name": "@rxstack/socketio-server",
  "version": "0.1.1",
  "description": "RxStack SocketIO Server Module",
  "author": "Nikolay Gergiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/rxstack.git"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "rxstack",
    "socketio"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "dependencies": {
    "@rxstack/async-event-dispatcher": "^0.1.1",
    "@rxstack/core": "^0.1.1",
    "@rxstack/exceptions": "^0.1.2",
    "injection-js": "^2.1.0",
    "socket.io": "^2.0.4",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@rxstack/utils": "^0.1.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.0.66",
    "@types/socket.io": "^1.4.36",
    "chai": "3.5.0",
    "istanbul": "0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^11.0.3",
    "should": "^13.2.3",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.9.1",
    "typescript": "^3.0",
    "socket.io-client": "^2.0.4"
  },
  "bugs": {
    "url": "https://github.com/rxstack/rxstack/issues"
  },
  "homepage": "https://github.com/rxstack/rxstack",
  "files": [
    "dist",
    "README.md",
    "./../../LICENSE"
  ],
  "nyc": {
    "include": [
      "./src/**/*.ts"
    ],
    "exclude": [
      "./test/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
