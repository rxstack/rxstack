{
  "name": "@rxstack/platform",
  "version": "0.1.9",
  "description": "RxStack Platform Module",
  "private": false,
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/rxstack.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "rxstack",
    "platform",
    "query-filter"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "peerDependencies": {
    "@rxstack/async-event-dispatcher": "^0.1",
    "@rxstack/core": "^0.1",
    "@rxstack/exceptions": "^0.1",
    "@rxstack/security": "^0.1",
    "@rxstack/query-filter": "^0.1"
  },
  "dependencies": {
    "injection-js": "^2.1",
    "reflect-metadata": "^0.1.13",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^2.2.48",
    "chai": "3.5.0",
    "mocha": "^5.2.0",
    "nyc": "^11.0.3",
    "should": "^13.2.3",
    "sinon": "^6.3.5",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333"
  },
  "bugs": {
    "url": "https://github.com/rxstack/rxstack/issues"
  },
  "homepage": "https://github.com/rxstack/rxstack",
  "files": [
    "dist",
    "README.md",
    "./../../LICENSE"
  ],
  "nyc": {
    "include": [
      "./src/**/*.ts"
    ],
    "exclude": [
      "./test/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
