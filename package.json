    "@rxstack/security": "^0.4",
    "@rxstack/service-registry": "^0.4",
    "@rxstack/socketio-server": "^0.4",
    "@rxstack/utils": "^0.4",
    "@rxstack/worker-threads-pool": "^0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^2.2.48",
    "@types/node": "^12.7.1",
    "chai": "3.5.0",
    "lerna": "^3.15.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "source-map-support": "^0.4.15",
